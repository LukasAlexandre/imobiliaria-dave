generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produto {
  id              String  @id @default(auto()) @map("_id") @db.ObjectId
  titulo          String
  descricao       String
  banheiros       Int
  metragemCasa    Int?
  metragemTerreno Int?
  observacao      String?
  foto01          String?
  foto02          String?
  foto03          String?
  foto04          String?
  foto05          String?
  foto06          String?
  foto07          String?
  foto08          String?
  foto09          String?
  foto10          String?
  garagem         Int
  localizacao     String
  preco           Float
  quartos         Int
  tipo            String
}
