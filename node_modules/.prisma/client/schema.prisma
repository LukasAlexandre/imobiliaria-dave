generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Produto {
  id              Int     @id @default(autoincrement())
  titulo          String
  descricao       String
  descricaoPrevia String
  preco           Float
  quartos         Int
  banheiros       Int
  garagem         Int
  localizacao     String
  tipo            String
  metragemCasa    Int
  metragemTerreno Int?
  status          String
  observacao      String?
  images          String
}
