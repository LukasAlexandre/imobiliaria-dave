generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produto {
  id          String  @id @default(auto()) @map("_id") @db.ObjectId
  titulo      String?
  descricao   String?
  quartos     Int
  banheiros   Int
  garagem     Int
  preco       Float
  metragem    Float?
  localizacao String?
  foto01      String?
  foto02      String?
  foto03      String?
  foto04      String?
  foto05      String?
  foto06      String?
  foto07      String?
  foto08      String?
  foto09      String?
  foto10      String?
}
