generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model produto {
  id              Int     @id @default(autoincrement())
  titulo          String  @db.VarChar(255)
  descricao       String  @db.Text
  quartos         Int
  banheiros       Int
  garagem         Int
  preco           Decimal @db.Decimal(10, 2)
  localizacao     String  @db.VarChar(255)
  tipo            String  @db.VarChar(255)
  metragemCasa    Int?
  metragemTerreno Int?
  observacao      String? @db.Text
  foto01          String? @db.VarChar(255)
  foto02          String? @db.VarChar(255)
  foto03          String? @db.VarChar(255)
  foto04          String? @db.VarChar(255)
  foto05          String? @db.VarChar(255)
  foto06          String? @db.VarChar(255)
  foto07          String? @db.VarChar(255)
  foto08          String? @db.VarChar(255)
  foto09          String? @db.VarChar(255)
  foto10          String? @db.VarChar(255)
}
