generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Produto {
  id        String @id @default(auto()) @map("_id") @db.ObjectId
  descricao   String
  quartos     Int
  banheiros   Int
  garagem     Int
  preco       Float
  foto01      String?
  foto02      String?
  foto03      String?
  foto04      String?
  foto05      String?
  foto06      String?
  foto07      String?
  foto08      String?
  foto09      String?
  foto10      String?
}