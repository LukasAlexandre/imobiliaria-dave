generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Produto {
  id              Int     @id @default(autoincrement())
  titulo          String
  descricao       String
  quartos         Int
  banheiros       Int
  garagem         Int
  preco           Float
  localizacao     String
  tipo            String
  metragemCasa    Int?
  metragemTerreno Int?
  observacao      String? @db.LongText
  descricaoPrevia String
  status          String
}
