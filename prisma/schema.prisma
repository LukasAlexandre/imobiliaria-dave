generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"   // ðŸŸ¢ Confirme que estÃ¡ como "mysql"
  url      = env("DATABASE_URL")
}

model Produto {
  id              Int     @id @default(autoincrement()) 
  titulo          String
  descricao       String
  quartos         Int
  banheiros       Int
  garagem         Int
  preco           Float
  localizacao     String
  tipo            String
  metragemCasa    Int?
  metragemTerreno Int?
  observacao      String?
  foto01          String?
  foto02          String?
  foto03          String?
  foto04          String?
  foto05          String?
  foto06          String?
  foto07          String?
  foto08          String?
  foto09          String?
  foto10          String?
}
