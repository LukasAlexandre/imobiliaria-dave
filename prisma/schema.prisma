model produto {
  id              String   @id @default(cuid())
  titulo          String
  descricao       String
  observacoes     String?
  quartos         Int
  banheiros       Int
  garagem         Int
  preco           Float
  metragemCasa    Float?
  metragemTerreno Float?
  localizacao     String
  tipo            String
  foto01          String?
  foto02          String?
  foto03          String?
  foto04          String?
  foto05          String?
  foto06          String?
  foto07          String?
  foto08          String?
  foto09          String?
  foto10          String?
  createdAt       DateTime @default(now())
}
